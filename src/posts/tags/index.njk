---
title: Tags
---
<ul class="tag-list">
{% for tag in collections.tagList %}
  {% set tagUrl %}/posts/tags/{{ tag.tag | slug }}.html{% endset %}
  <li class="tag-list__item row"><a href="{{ tagUrl | url }}">{{ tag.tag }}</a>  <span class="rowitem--right">{{ tag.count }}</span></li>
{% endfor %}
</ul>
